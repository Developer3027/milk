<main class="bg-input-border-100">
  <div class="w-full">
    <% if notice.present? %>
      <p class="py-2 px-3 bg-green-50 mb-5 text-green-500 font-medium rounded-lg inline-block" id="notice"><%= notice %></p>
    <% end %>

    <div id="eruditions" class="min-w-full">
      <%= render @eruditions %>
    </div>
  </div>
</main>
